{
  "name": "ts-webpack-template",
  "version": "1.0.0",
  "description": "typescript + react + webpack template",
  "main": "./src/index.tsx",
  "scripts": {
    "start": "cross-env NODE_ENV=development webpack-dev-server --progress --mode development --config ./config/webpack.dev.config.js",
    "bundle": "cross-env NODE_ENV=production webpack            --progress --mode production --config ./config/webpack.dev.bundle-analyzer.config.js",
    "build": "cross-env NODE_ENV=production  webpack            --progress --mode production --config ./config/webpack.prod.config.js",
    "lint": "eslint src --ext .js --ext .ts --ext .jsx --ext .tsx",
    "lint-fix": "eslint src --fix --ext .js --ext .ts --ext .jsx --ext .tsx"
  },
  "dependencies": {
    "antd": "^3.20.0",
    "react": "^16.8.6",
    "react-dom": "^16.8.6"
  },
  "devDependencies": {
    "@types/antd": "^1.0.0",
    "@types/react": "^16.8.22",
    "@types/react-dom": "^16.8.4",
    "@typescript-eslint/eslint-plugin": "^1.11.0",
    "@typescript-eslint/parser": "^1.11.0",
    "autoprefixer": "^9.5.1",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "clean-webpack-plugin": "^2.0.1",
    "cross-env": "^5.2.0",
    "css-loader": "^1.0.1",
    "eslint": "^6.0.1",
    "eslint-config-airbnb-base": "^13.2.0",
    "eslint-import-resolver-webpack": "^0.11.1",
    "eslint-loader": "^2.1.2",
    "eslint-plugin-import": "^2.18.0",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.14.2",
    "file-loader": "^3.0.1",
    "html-webpack-plugin": "^3.2.0",
    "json-loader": "^0.5.7",
    "mini-css-extract-plugin": "^0.6.0",
    "node-sass": "^4.12.0",
    "optimize-css-assets-webpack-plugin": "^5.0.1",
    "postcss-loader": "^3.0.0",
    "raw-loader": "^3.0.0",
    "sass-loader": "^7.1.0",
    "source-map-loader": "^0.2.4",
    "style-loader": "^0.23.1",
    "ts-import-plugin": "^1.5.5",
    "ts-loader": "^5.4.5",
    "typescript": "^3.4.5",
    "typings-for-css-modules-loader": "^1.7.0",
    "uglifyjs-webpack-plugin": "^2.1.2",
    "url-loader": "^1.1.2",
    "webpack": "^4.30.0",
    "webpack-bundle-analyzer": "^3.3.2",
    "webpack-cli": "^3.3.1",
    "webpack-dev-server": "^3.3.1",
    "webpack-merge": "^4.2.1"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "author": "1038761793@qq.com",
  "license": "MIT"
}
